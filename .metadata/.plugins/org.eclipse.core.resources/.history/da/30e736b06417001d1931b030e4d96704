package enquanto;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		
		while(x == 0) {
			System.out.println("agora foi zero");			
			
		}
		
		System.out.println("NÃ£o era zero");
		x = sc.nextInt();
		
		sc.close();
		

	}

}
